{
  "name": "@bakesaled/mesh-jack",
  "version": "0.0.1",
  "description": "Decoupled Angular message bus.",
  "main": "release/index.js",
  "module": "release/esm.js",
  "typings": "release/index.d.ts",
  "keywords": [
    "angular",
    "typescript",
    "message bus"
  ],
  "author": "Brandon Eleuterio",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bakesaled/mesh-jack.git"
  },
  "bugs": {
    "url": "https://github.com/bakesaled/mesh-jack/issues"
  },
  "homepage": "https://github.com/bakesaled/mesh-jack#readme",
  "engines": {
    "node": ">= 10.6.0"
  },
  "files": [
    "release"
  ],
  "scripts": {
    "clean": "npm-run-all -p clean:*",
    "clean:build": "rimraf build",
    "clean:dist": "rimraf dist",
    "clean:release": "rimraf release",
    "copy:build": "cpx \"projects/bus/src/**/!(test.ts|bus.service.spec.ts)\" build",
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:esm": "rollup --config rollup.config.js",
    "build:package": "npm run build:esm",
    "package": "npm-run-all -s clean copy:build package:aot build:package package:minify",
    "package:aot": "ngc -p tsconfig-aot.json",
    "package:minify": "uglifyjs release/index.js --source-map --compress --mangle --screw-ie8 --output release/index.min.js",
    "test": "ng test",
    "test:all": "ng test all",
    "test:all:watch": "npm run test:all -- --watch",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "release": "standard-version"
  },
  "dependencies": {
    "@angular/animations": "~8.2.0",
    "@angular/common": "~8.2.0",
    "@angular/compiler": "~8.2.0",
    "@angular/core": "~8.2.0",
    "@angular/forms": "~8.2.0",
    "@angular/platform-browser": "~8.2.0",
    "@angular/platform-browser-dynamic": "~8.2.0",
    "@angular/router": "~8.2.0",
    "rxjs": "~6.4.0",
    "tslib": "^1.10.0",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.802.2",
    "@angular-devkit/build-ng-packagr": "~0.802.2",
    "@angular/cli": "~8.2.2",
    "@angular/compiler-cli": "~8.2.0",
    "@angular/language-service": "~8.2.0",
    "@types/jasmine": "~3.3.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "^5.0.0",
    "cpx": "^1.5.0",
    "jasmine-core": "~3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.1.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "ng-packagr": "^5.3.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.18.2",
    "protractor": "~5.4.0",
    "rimraf": "^3.0.0",
    "rollup": "^1.19.4",
    "rollup-plugin-angular": "^0.5.3",
    "rollup-plugin-commonjs": "^10.0.2",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-typescript": "^1.0.1",
    "standard-version": "^7.0.0",
    "ts-node": "~7.0.0",
    "tsickle": "^0.36.0",
    "tslint": "~5.15.0",
    "typescript": "~3.5.3",
    "uglify-js": "^3.6.0"
  },
  "peerDependencies": {
    "@angular/common": "8.x",
    "@angular/core": "8.x",
    "rxjs": "6.x"
  },
  "resolutions": {
    "minimatch": "^3.0.2"
  }
}
